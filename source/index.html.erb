<div class="contact-form padded" id="contact">
    <div class="container">
    <p>Contactez-nous pour un devis gratuit</p>
      <form class="form-horizontal" action="https://formspree.io/contact@skypicture.fr"
            method="POST">
          <div class="form-group">
                  <div class="col-sm-12">
                    <input type="text" class="form-control" name="name" id="inputEmail3" placeholder="Mon nom"" ">
                  </div>
                </div>
              <div class="form-group">

                  <div class="col-xs-12">
                    <input type="text" class="form-control" name="subject" id="inputEmail3" placeholder="Sujet">
                  </div>

                </div>
                <div class="form-group">

                  <div class="col-xs-12">
                    <input type="email" class="form-control" name="email" id="inputEmail3" placeholder="Email">
                  </div>
                </div>
                <div class="form-group">

                  <div class="col-xs-12">
                    <textarea type="text" class="form-control" name="message" id="inputText" rows="10" placeholder="Lieux de tournage, durée du tournage, budget, etc..."></textarea>
                  </div>
                </div>
                <div class="form-group">
                  <div class="col-xs-12">
                    <button type="submit" class="btn btn-primary">Envoyer le mail</button>
                  </div>
                </div>
              </form>

    </div>
</div>


